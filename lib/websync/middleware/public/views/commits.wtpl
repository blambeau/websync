?{!commits.empty?}{
<table class="commits">
  <thead>
    <tr>
      <th width="150px">date</th>
      <th width="150px">author</th>
      <th>summary</th>
    </tr>
  </thead>
  *{commits using each_with_index as c,i}{
  <tbody>
    <tr ?{i%2 != 0}{class="odd"}>
      <td width="150px">${c.date}</td>
      <td width="150px">${c.author.name}</td>
      <td>${c.short_message}</td>
    </tr>
  </tbody>
  }
</table>}
